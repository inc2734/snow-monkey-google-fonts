{
	"name": "inc2734/snow-monkey-google-fonts",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"require": {
		"php": ">=7.4",
		"inc2734/wp-github-plugin-updater": "^3.1"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^2.3",
		"dealerdirect/phpcodesniffer-composer-installer": "^1.0",
		"yoast/phpunit-polyfills": "^1.0"
	},
	"authors": [
		{
			"name": "Takashi Kitajima",
			"email": "inc@2inc.org"
		}
	],
	"autoload": {
		"psr-4" : {
			"Snow_Monkey\\Plugin\\GoogleFonts\\" : ""
		}
	},
	"scripts": {
		"lint": "./vendor/squizlabs/php_codesniffer/bin/phpcs",
		"format": "./vendor/squizlabs/php_codesniffer/bin/phpcbf"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
